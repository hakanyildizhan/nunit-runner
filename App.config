<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="defaultListener" type="NUnitRunner.TraceListener, NUnitRunner" initializeData="NUnitRunner.log" />
        <add name="consoleListener" type="NUnitRunner.ConsoleListener, NUnitRunner"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>